{
  "name": "@jianxing/jx",
  "type": "module",
  "version": "0.0.6",
  "packageManager": "pnpm@9.1.1",
  "description": "_description_",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/jianxing-xu",
  "homepage": "https://github.com/jianxing-xu/jx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jianxing-xu/jx.git"
  },
  "bugs": "https://github.com/jianxing-xu/jx/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./dist/index.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "adm-zip": "^0.5.14",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ini": "^4.1.3",
    "inquirer": "^9.3.1",
    "inquirer-autocomplete-standalone": "^0.8.1",
    "node-fetch": "^3.3.2",
    "octokit": "^4.0.2",
    "ora": "^8.0.1",
    "progress": "^2.0.3",
    "zx": "^8.1.3"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.8",
    "@jianxing/eslint-config": "^1.0.3",
    "@types/adm-zip": "^0.5.5",
    "@types/ini": "^4.1.1",
    "@types/node": "^20.12.12",
    "bumpp": "^9.4.1",
    "eslint": "^9.3.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.1",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
